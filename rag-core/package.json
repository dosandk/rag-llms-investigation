    "cheerio": "^1.0.0-rc.12",
    "chromadb": "^1.8.1",
    "chromadb-default-embed": "^2.13.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "langchain": "^0.2.12",
    "readline-sync": "^1.4.10",
    "unstructured-client": "^0.14.0"
